#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main()
{
	int number[10] = { 0, };
	int count=0, save;

	scanf("%d", &count);
	for (int i = 0; i < count; i++)
	{
		scanf("%d", &number[i]);
	}

	for(int i=0;i<count;i++)
		for (int j = 0; j < count; j++)
		{
			if (number[i] < number[j])
			{
				save = number[i];
				number[i] = number[j];
				number[j] = save;
			}
		}

	for (int i = 0; i < count; i++)
	{
		printf("%d", number[i]);
	}
}
